.contact-button {
  background: none;
  margin: 0;
  padding: 0;
  font-size: 0;
  width: 100rpx;
}

.contact-button::after {
  display: none;
}

/**基本框架样式**/

.rec-img, .ads-img, .swiper-box, .goods-img {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}

.search {
  -webkit-transition: all 0.5s ease;
  transition: all 0.5s ease;
}

.swiper {
  width: 100%;
  position: relative;
  padding-bottom: 50%;
}

.swiper-img {
  width: 100%;
  height: 100%;
}

page .wx-swiper-dot {
  content: '';
  width: 14rpx;
  height: 14rpx;
  border-radius: 50rpx;
}

page .wx-swiper-dot:nth-child(n) {
  margin-right: 10rpx;
}

page .wx-swiper-dots.wx-swiper-dots-horizontal {
  bottom: 0;
  overflow: visible;
  font-size: 0;
  padding: 0 2rpx;
}

.search {
  position: fixed;
  left: 0;
  top: 0;
  padding: 16rpx 0;
  width: 100%;
  z-index: 9;
  background: -webkit-linear-gradient(top, rgba(0, 0, 0, 0.7), rgba(0, 0, 0, 0));
  background: linear-gradient(top, rgba(0, 0, 0, 0.7), rgba(0, 0, 0, 0));
}

.search-box {
  background: #fff;
  border-radius: 100rpx;
  width: 100%;
  opacity: 0.9;
}

.search-input {
  height: 64rpx;
  padding: 0 20rpx;
  text-align: center;
  vertical-align: top;
}

button.search-btn {
  background: none;
  padding: 0 0.8em;
}

button.search-btn icon {
  vertical-align: top;
  margin-top: 15rpx;
}

.search-btn::after {
  display: none;
}

.icon-saoyisao, .icon-mark {
  width: 100rpx;
  text-align: center;
  line-height: 64rpx;
  color: #fff;
  font-size: 48rpx;
  text-shadow: 1px 1px 3px rgba(0, 0, 0, 0.2);
}

.search-active .search-box {
  opacity: 1;
}

.menu {
  background: #fff;
  /* padding: 30rpx 0; */
  height: 390rpx;
}

.menu-item {
  width: 20%;
  float: left;
  font-size: 26rpx;
}

.menu-item:nth-child(n+6) {
  margin-top: 20rpx;
}

.menu-item-img {
  width: 78rpx;
  height: 78rpx;
  border-radius: 50%;
  margin: 0 auto;
  margin-bottom: 8rpx;
  display: block;
}

.ads {
  margin: 16rpx;
}

.ads-item {
  position: relative;
}

.ads-1th .ads-item {
  width: 100%;
  padding-bottom: 50%;
}

.ads-2th .ads-item {
  width: 49%;
  padding-bottom: 24.5%;
  float: left;
  margin-left: 2%;
}

.ads-2th .ads-item:first-child {
  margin-left: 0;
}

.ads-3th .ads-item {
  width: 49%;
  padding-bottom: 23.5%;
  float: left;
  margin-left: 2%;
}

.ads-3th .ads-item:first-child {
  margin-left: 0;
  padding-bottom: 49%;
}

.ads-3th .ads-item:last-child {
  margin-top: 2%;
}

.home-tit {
  background: #fff;
  margin-top: 16rpx;
  padding: 20rpx 0;
}

.rec-imgbox {
  width: 100%;
  /* padding-bottom: 100%; */
  position: relative;
  /* border:1px solid red; */
}

.good-name, .goods-name {
  /* margin: 15rpx 0 10rpx; */
  height: 60rpx;
  font-size: 26rpx;
  line-height: 30rpx;
  /* border: 1px solid red; */
}

.home-rec {
  width: 750rpx;
  height: 250rpx;
  background: #fff;
  padding: 20rpx 16rpx;
}
.rec-item{
  width: 250rpx;
  height: 250rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  /* border: 1px solid red; */
}
/* .rec-item:nth-child(2) {
  margin: 0 16rpx;
} */

.delLine {
  color: #b4b4b4;
  margin-left: 10rpx;
  font-size: 24rpx;
}

.goods-tit {
  margin-top: 16rpx;
  padding: 20rpx 0;
  position: relative;
}

.goods-tit::after, .goods-tit::before {
  content: "";
  position: absolute;
  display: block;
  border-top: 1px solid #ececec;
  top: 50%;
  width: calc(50% - 120rpx);
}

.goods-tit::after {
  left: 0;
}

.goods-tit::before {
  right: 0;
}

.goods {
  padding: 0 16rpx 16rpx;
}

.goods-item {
  width: 49%;
  background: #fff;
  float: left;
}

.goods-item:nth-child(2n) {
  margin-left: 2%;
}

.goods-item:nth-child(n+3) {
  margin-top: 2%;
}

.goods-imgbox {
  position: relative;
  width: 100%;
  padding-bottom: 100%;
}

.goods-info {
  margin: 10rpx;
}

.card-item {
  position: relative;
  width: 42.5%;
  background: #fe8a0d;
  padding-bottom: 20%;
  margin-left: 5%;
  float: left;
  color: #fff;
  overflow: hidden;
}

.card-item:nth-child(n+3) {
  margin-top: 3%;
}

.card-item:after, .card-item:before {
  content: "";
  display: block;
  width: 30rpx;
  height: 200%;
  position: absolute;
  top: 50%;
  bottom: 0;
  background: radial-gradient(#f5f5f5, #f5f5f5 12rpx, rgba(0, 0, 0, 0) 12rpx);
  background-size: 32rpx 32rpx;
}

.card-item:after {
  left: 0;
  -webkit-transform: translate(-50%, -50%) scale(0.5);
  transform: translate(-50%, -50%) scale(0.5);
}

.card-item:before {
  right: 0;
  -webkit-transform: translate(50%, -50%) scale(0.5);
  transform: translate(50%, -50%) scale(0.5);
}

.card-info {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.card-txt {
  line-height: 120%;
  padding: 0 25rpx 0 19rpx;
  position: relative;
}

.card-txt-box {
  display: block;
  width: 28rpx;
}

.card-count {
  border-right: 1px dashed #fff;
  overflow: hidden;
  position: relative;
  margin-left: 30rpx;
  padding-right: 30rpx;
}

.card-txt::after, .card-txt::before {
  content: "";
  width: 18rpx;
  height: 18rpx;
  display: block;
  background: #f5f5f5;
  border-radius: 50%;
  position: absolute;
  left: -8rpx;
}

.card-txt::after {
  top: -9rpx;
}

.card-txt::before {
  bottom: -9rpx;
}

.card-counts {
  padding-bottom: 12rpx;
  margin-bottom: 12rpx;
}

.cards {
  width: 100%;
  text-align: right;
}

.card {
  margin: 30rpx 0;
}

.card-count, .card-txt {
  display: -webkit-box;
  -webkit-box-align: center;
}

.card-tit {
  background: #fff;
  font-size: 32rpx;
  padding: 20rpx 30rpx;
  line-height: 48rpx;
  color: #fe8a0d;
}

.icon-ticket {
  background: #fe8a0d;
  color: #fff;
  width: 62rpx;
  height: 62rpx;
  line-height: 62rpx;
  text-align: center;
  border-radius: 50%;
  font-size: 38rpx;
  display: inline-block;
  overflow: hidden;
  -webkit-transform: scale(0.6);
  transform: scale(0.6);
  -webkit-transform: left center;
  transform: left center;
  margin: -15rpx 0;
}

.tips-foot {
  padding: 20rpx 0;
  color: #b4b4b4;
  position: relative;
}

.tips-foot::after, .tips-foot::before {
  content: "";
  position: absolute;
  display: block;
  border-top: 1px solid #ececec;
  top: 50%;
  width: calc(50% - 160rpx);
}

.tips-foot::after {
  left: 0;
}

.tips-foot::before {
  right: 0;
}

