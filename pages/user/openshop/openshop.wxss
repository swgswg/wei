.content{
    width: 100%;
    height: 1205rpx;
    padding: 20rpx;
    box-sizing: border-box;
    /* border: 1px solid red; */
}
.title{
    font-size: 30rpx;
}
.from-each{
    margin-top:20rpx;
}
.from-each-title{
    font-size:30rpx;
}
.from-each-input{
    width: 100%;
    height: 100%;
    padding-left:20rpx;
    box-sizing: border-box; 
    font-size:26rpx; 
}
input{
    width:100%;
    height: 70rpx;
    border:1rpx solid #ddd;
}
textarea{
    width:100%;
    height:200rpx;
    border:1rpx solid #aaa;
}
.from-each-image{
    width: 100%;
    text-align: center;
}
.from-each-image image{
    width: 700rpx;
    height: 460rpx;
}
button{
    margin-top:20rpx;
}