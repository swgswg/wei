/* pages/template/template.wxss */
.icon{
    width:44rpx;
    height: 44rpx;
}
.tabBar{
    position: fixed;
    display: flex;
    justify-content: space-around;
    align-items: center;
    left: 0;
    bottom:0;
    z-index: 99999999;
    width:100%;
    height: 98rpx;
    padding: 0 10rpx;
    box-sizing: border-box; 
    background:#F7F7FA;
    font-size:24rpx;
    color:#8A8A8A;
    box-shadow: 6rpx 6rpx 6rpx 6rpx #aaa;
}
.tabBar-item navigator{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    width:150rpx;
    height: 100rpx;
    padding-top: 11rpx;
    box-sizing: border-box;
     
}
/*当前字体颜色*/
.tabBartext{
    margin-top:-5rpx;  
    /* color:red; */
}

.tabbar_active{
    color: red;
}